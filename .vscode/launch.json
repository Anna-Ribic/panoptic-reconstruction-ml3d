    "version": "0.2.0",
    "configurations": [
        {
            "name": "Joined Infer",
            "type": "python",
            "request": "launch",
            "program": "tools/test_joined_single_image.py",
            "cwd": "${workspaceFolder}",
            "env": {"PYTHONPATH": "${workspaceFolder}${pathSeparator}${env:PYTHONPATH}"},
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-i", "data/front3d/3f48e194-1e88-4dc1-b1d2-03114248c15f/rgb_0008.png",
                "-o", "outputs/joined_infer/3f48e194-1e88-4dc1-b1d2-03114248c15f/rgb_0008"
            ]
        },
        {
            "name": "Panoptic Infer",
            "type": "python",
            "request": "launch",
            "program": "tools/test_net_single_image.py",
            "cwd": "${workspaceFolder}",
            "env": {"PYTHONPATH": "${workspaceFolder}${pathSeparator}${env:PYTHONPATH}"},
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-i", "data/front3d/0a9c667d-033d-448c-b17c-dc55e6d3c386/rgb_0019.png",
                "-o", "outputs/front3d_samples"
            ]
        },
        {
            "name": "Train",
            "type": "python",
            "request": "launch",
            "program": "tools/train_panoptic_sdfusion.py",
            "cwd": "${workspaceFolder}",
            "env": {"PYTHONPATH": "${workspaceFolder}${pathSeparator}${env:PYTHONPATH}"},
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--config-file", "configs/front3d_train_joined.yaml",
                "--output-path", "outputs/train_joined"
            ]
        },
    ]
}